{
  "name": "regulated-professions-register",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:assets": "encore dev",
    "build:assets:watch": "encore dev --watch",
    "build:assets:prod": "encore production",
    "console": "ts-node --transpile-only ./src/console.ts",
    "heroku-postbuild": "npm run build && npm run build:assets:prod",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run build && npm run typeorm -- migration:generate -d ormconfig.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create",
    "typeorm:migration:run": "npm run build && npm run typeorm -- migration:run -d ormconfig.ts",
    "typeorm:migration:revert": "npm run build && npm run typeorm -- migration:revert -d ormconfig.ts",
    "data:migrate:generate": "ts-node ./util/generate-data-migration.ts",
    "data:migrate": "ts-node --transpile-only",
    "format:check": "prettier --check . --ignore-unknown",
    "format": "prettier --write . --ignore-unknown",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "NODE_ENV=test nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,cypress}/**/*.{ts,js}\" --fix --max-warnings=0",
    "cy:run": "cypress run",
    "cypress:open": "cypress open",
    "cy:open": "start-server-and-test start:test http://localhost:3000 cypress:open",
    "test": "jest",
    "test:ci": "npm run format:check && npm run lint && npm run test:cov",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "start-server-and-test start:test http://localhost:3000 cy:run",
    "test:e2e:ci": "start-server-and-test start:test http://localhost:3000 \"npm run cy:run -- --spec=$SPECS\"",
    "seed": "node dist/seeder",
    "seed:refresh": "npm run build && node dist/seeder --refresh",
    "seed:test": "NODE_ENV=test node dist/seeder",
    "seed:production": "NODE_ENV=production node dist/seeder",
    "seed:test:refresh": "npm run build && NODE_ENV=test node dist/seeder --refresh",
    "opensearch:purge": "curl -X DELETE 'http://admin:admin@localhost:9200/professions_development' &> /dev/null && curl -X DELETE 'http://admin:admin@localhost:9200/organisations_development' &> /dev/null",
    "opensearch:test:purge": "curl -X DELETE 'http://admin:admin@localhost:9200/professions_test' &> /dev/null && curl -X DELETE 'http://admin:admin@localhost:9200/organisations_test' &> /dev/null",
    "opensearch:reseed:professions": "npm run console -- opensearch:reseed:professions",
    "opensearch:reseed:organisations": "npm run console --  opensearch:reseed:organisations",
    "heroku:set-callback-urls": "ts-node util/set-callback-urls.ts",
    "bull:repl": "bull-repl"
  },
  "dependencies": {
    "@nestjs/bull": "^10.2.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/typeorm": "^10.0.2",
    "@opensearch-project/opensearch": "^2.12.0",
    "@symfony/webpack-encore": "^5.0.0",
    "@types/auth0": "^3.3.10",
    "@types/connect-flash": "^0.0.40",
    "@types/express-session": "^1.18.0",
    "@types/google-libphonenumber": "^7.4.30",
    "auth0": "^4.10.0",
    "axios": "^1.7.7",
    "bull": "^4.16.3",
    "bull-repl": "^1.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^7.1.1",
    "core-js": "^3.38.1",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.1",
    "date-fns": "^4.1.0",
    "express-basic-auth": "^1.2.1",
    "express-openid-connect": "^2.17.1",
    "express-session": "^1.18.0",
    "file-loader": "^6.2.0",
    "google-libphonenumber": "^3.2.38",
    "govuk-frontend": "^5.6.0",
    "jwt-decode": "^4.0.0",
    "method-override": "^3.0.0",
    "nestjs-i18n": "^10.4.9",
    "nestjs-opensearch": "^0.4.0",
    "nestjs-seeder": "^0.3.2",
    "notifications-node-client": "^8.2.1",
    "nunjucks": "^3.2.4",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rollbar": "^2.26.4",
    "rxjs": "^7.5.5",
    "sass": "^1.79.4",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  },
  "overrides": {
    "path-to-regexp": "^0.1.10",
    "body-parser": "^1.20.3"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.6",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/jsdom": "^21.1.7",
    "@types/method-override": "^0.0.35",
    "@types/node": "^22.7.4",
    "@types/nunjucks": "^3.2.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "cypress": "^13.15.0",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-when": "^3.6.0",
    "prettier": "^3.3.3",
    "puppeteer": "^23.4.1",
    "source-map-support": "^0.5.20",
    "start-server-and-test": "^2.0.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/db/migrate/"
    ],
    "rootDir": "src",
    "roots": [
      "",
      "../views"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/main.ts",
      "!**/seeder.ts",
      "!**/*.module.ts",
      "!**/*.module.ts",
      "!**/*.config.ts",
      "!**/*.seeder.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./testutils/jest.setup.ts"
    ]
  }
}
