{
  "name": "regulated-professions-register",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:assets": "encore dev",
    "build:assets:watch": "encore dev --watch",
    "build:assets:prod": "encore production",
    "console": "ts-node --transpile-only ./src/console.ts",
    "heroku-postbuild": "npm run build && npm run build:assets:prod",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run build && npm run typeorm -- migration:generate -n",
    "typeorm:migration:create": "npm run typeorm -- migration:create -n",
    "typeorm:migration:run": "npm run build && npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run build && npm run typeorm -- migration:revert",
    "data:migrate:generate": "ts-node ./util/generate-data-migration.ts",
    "data:migrate": "ts-node --transpile-only",
    "format:check": "prettier --check . --ignore-unknown",
    "format": "prettier --write . --ignore-unknown",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "NODE_ENV=test nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,cypress}/**/*.{ts,js}\" --fix --max-warnings=0",
    "cy:run": "cypress run",
    "cypress:open": "cypress open",
    "cy:open": "start-server-and-test start:test http://localhost:3000 cypress:open",
    "test": "jest",
    "test:ci": "npm run format:check && npm run lint && npm run test:cov",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "start-server-and-test start:test http://localhost:3000 cy:run",
    "test:e2e:ci": "start-server-and-test start:test http://localhost:3000 \"npm run cy:run -- --spec=$SPECS\"",
    "seed": "node dist/seeder",
    "seed:refresh": "npm run build && node dist/seeder --refresh",
    "seed:test": "NODE_ENV=test node dist/seeder",
    "seed:production": "NODE_ENV=production node dist/seeder",
    "seed:test:refresh": "npm run build && NODE_ENV=test node dist/seeder --refresh",
    "opensearch:purge": "curl -X DELETE 'http://admin:admin@localhost:9200/professions_development' &> /dev/null && curl -X DELETE 'http://admin:admin@localhost:9200/organisations_development' &> /dev/null",
    "opensearch:test:purge": "curl -X DELETE 'http://admin:admin@localhost:9200/professions_test' &> /dev/null && curl -X DELETE 'http://admin:admin@localhost:9200/organisations_test' &> /dev/null",
    "opensearch:reseed:professions": "npm run console -- opensearch:reseed:professions",
    "opensearch:reseed:organisations": "npm run console --  opensearch:reseed:organisations",
    "heroku:set-callback-urls": "ts-node util/set-callback-urls.ts",
    "bull:repl": "bull-repl"
  },
  "dependencies": {
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/typeorm": "^8.0.5",
    "@opensearch-project/opensearch": "^1.1.0",
    "@symfony/webpack-encore": "^2.1.0",
    "@types/auth0": "^2.35.0",
    "@types/connect-flash": "^0.0.37",
    "@types/connect-redis": "^0.0.18",
    "@types/express-session": "^1.17.4",
    "@types/google-libphonenumber": "^7.4.23",
    "auth0": "^4.3.1",
    "bull": "^4.12.2",
    "bull-repl": "^1.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.1.3",
    "csv-parse": "^5.1.0",
    "csv-stringify": "^6.1.0",
    "date-fns": "^2.28.0",
    "express-basic-auth": "^1.2.1",
    "express-openid-connect": "^2.7.2",
    "express-session": "^1.17.3",
    "file-loader": "^6.2.0",
    "google-libphonenumber": "^3.2.28",
    "govuk-frontend": "^4.8.0",
    "jwt-decode": "^3.1.2",
    "method-override": "^3.0.0",
    "nestjs-i18n": "^10.2.6",
    "nestjs-opensearch": "^0.3.2",
    "nestjs-seeder": "^0.2.0",
    "notifications-node-client": "^8.0.0",
    "nunjucks": "^3.2.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollbar": "^2.25.0",
    "rxjs": "^7.5.5",
    "sass": "^1.54.2",
    "sass-loader": "^12.6.0",
    "slugify": "^1.6.5",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.3",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^10.3.7",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/method-override": "^0.0.32",
    "@types/node": "^20.8.8",
    "@types/nunjucks": "^3.2.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.27.1",
    "cypress": "^9.7.0",
    "cypress-axe": "^0.14.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fishery": "^2.2.2",
    "jest": "^27.5.1",
    "jest-when": "^3.5.1",
    "prettier": "^2.7.1",
    "puppeteer": "^14.3.0",
    "source-map-support": "^0.5.20",
    "start-server-and-test": "^2.0.3",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/db/migrate/"
    ],
    "rootDir": "src",
    "roots": [
      "",
      "../views"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/main.ts",
      "!**/seeder.ts",
      "!**/*.module.ts",
      "!**/*.module.ts",
      "!**/*.config.ts",
      "!**/*.seeder.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./testutils/jest.setup.ts"
    ]
  }
}
