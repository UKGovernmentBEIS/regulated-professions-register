{
  "name": "regulated-professions-register",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:assets": "encore dev",
    "build:assets:prod": "encore production",
    "heroku-postbuild": "npm run build && npm run build:assets:prod",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run build && npm run typeorm -- migration:generate -n",
    "typeorm:migration:create": "npm run typeorm -- migration:create -n",
    "typeorm:migration:run": "npm run build && npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run build && npm run typeorm -- migration:revert",
    "format:check": "prettier --check . --ignore-unknown",
    "format": "prettier --write . --ignore-unknown",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "NODE_ENV=test nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,cypress}/**/*.{ts,js}\" --fix --max-warnings=0",
    "cy:run": "cypress run",
    "cypress:open": "cypress open",
    "cy:open": "start-server-and-test start:test http://localhost:3000 cypress:open",
    "test": "jest",
    "test:ci": "npm run format:check && npm run lint && npm run test:cov",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "start-server-and-test start:test http://localhost:3000 cy:run",
    "test:e2e:ci": "start-server-and-test start:test http://localhost:3000 \"npm run cy:run -- --spec=$SPECS\"",
    "seed": "node dist/seeder",
    "seed:refresh": "npm run build && node dist/seeder --refresh",
    "seed:test": "NODE_ENV=test node dist/seeder",
    "seed:production": "NODE_ENV=production node dist/seeder",
    "seed:test:refresh": "npm run build && NODE_ENV=test node dist/seeder --refresh",
    "heroku:set-callback-urls": "ts-node util/set-callback-urls.ts",
    "bull:repl": "bull-repl"
  },
  "dependencies": {
    "@nestjs/bull": "^0.5.2",
    "@nestjs/cli": "^8.2.2",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/typeorm": "^8.0.3",
    "@symfony/webpack-encore": "^1.8.1",
    "@types/auth0": "^2.34.13",
    "@types/connect-flash": "^0.0.37",
    "@types/express-session": "^1.17.4",
    "auth0": "^2.40.0",
    "bull": "^4.7.0",
    "bull-repl": "^0.27.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-flash": "^0.1.1",
    "date-fns": "^2.28.0",
    "express-basic-auth": "^1.2.1",
    "express-openid-connect": "^2.7.1",
    "express-session": "^1.17.2",
    "file-loader": "^6.2.0",
    "govuk-frontend": "^4.0.1",
    "jwt-decode": "^3.1.2",
    "method-override": "^3.0.0",
    "nestjs-i18n": "^8.2.2",
    "nestjs-seeder": "^0.2.0",
    "notifications-node-client": "^5.1.1",
    "nunjucks": "^3.2.3",
    "pg": "^8.7.3",
    "plausible-tracker": "^0.3.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollbar": "^2.24.0",
    "rxjs": "^7.5.4",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "slugify": "^1.6.5",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.4.0",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/method-override": "^0.0.32",
    "@types/node": "^17.0.21",
    "@types/nunjucks": "^3.2.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cypress": "^9.5.1",
    "cypress-axe": "^0.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fishery": "^2.2.2",
    "jest": "^27.5.1",
    "jest-when": "^3.5.1",
    "prettier": "^2.5.1",
    "puppeteer": "^13.5.0",
    "source-map-support": "^0.5.20",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.6.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/db/migrate/"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/main.ts",
      "!**/seeder.ts",
      "!**/*.module.ts",
      "!**/*.module.ts",
      "!**/*.config.ts",
      "!**/*.seeder.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./testutils/jest.setup.ts"
    ]
  }
}
